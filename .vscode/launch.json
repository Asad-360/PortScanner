{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Debug sps",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/man",
            //"args": ["-s", "scanme.nmap.org", "-p", "80,22,9929,11211,31337"],
            "args": [
                "scanme.nmap.org"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}